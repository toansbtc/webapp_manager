generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["catho_schema"]
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model account_role {
  user_token String[] @unique @db.Char
  role       Int
  is_active  Boolean

  @@schema("catho_schema")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model image_path {
  id         Int      @unique @default(1)
  avatar_img String?  @db.Char
  user_token String[] @db.Char

  @@schema("catho_schema")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model infor {
  id            Int       @unique @default(1)
  user_token    String[]  @db.Char
  birth_day     DateTime? @db.Date
  name          String[]  @db.Char
  number_phone  String[]  @db.Char
  address       String[]  @db.Char
  situation     String[]  @db.Char
  job           String[]  @db.Char
  position      String[]  @db.Char
  self_introduc String[]  @db.Char

  @@schema("catho_schema")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model farther_intro {
  name         String?   @db.Char
  time_start   DateTime? @db.Date
  office       String?   @db.Char
  introduction String?   @db.Char
  id           Int       @unique @default(autoincrement())

  @@schema("catho_schema")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model intro_home {
  image_path     String[] @db.Char
  introduct_text String[] @db.Char
  id             Int      @unique @default(autoincrement())

  @@schema("catho_schema")
}
