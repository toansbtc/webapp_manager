generator client {
  provider = "prisma-client-js"
  // previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  // schemas  = ["catho_schema"]
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model account_role {
  role       Int
  is_active  Boolean
  user_token String  @id @db.VarChar(255)

  // @@schema("catho_schema")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model image_path {
  image_path    String?
  id_image_path Int             @id @unique @default(autoincrement())
  farther_intro farther_intro[]
  infor         infor[]
  intro_home    intro_home[]

  // @@schema("catho_schema")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model infor {
  birth_day         DateTime?   @db.Date
  name              String[]    @db.Char
  number_phone      String[]    @db.Char
  address           String[]    @db.Char
  situation         String[]    @db.Char
  job               String[]    @db.Char
  position          String[]    @db.Char
  self_introduc     String[]    @db.Char
  id_avata_img_path Int?
  user_token        String      @db.VarChar(255)
  id                Int         @id @unique @default(autoincrement())
  image_path        image_path? @relation(fields: [id_avata_img_path], references: [id_image_path], onDelete: Cascade, map: "fk_image_path")

  // @@schema("catho_schema")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model farther_intro {
  name          String?     @db.VarChar(255)
  time_start    DateTime?   @db.Date
  office        String?     @db.VarChar(255)
  introduction  String?
  id            Int         @id @default(autoincrement())
  id_image_path Int?
  image_path    image_path? @relation(fields: [id_image_path], references: [id_image_path], onDelete: Cascade, map: "fk_image_path")

  // @@schema("catho_schema")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model intro_home {
  id            Int         @id @default(autoincrement())
  id_image_path Int?
  introduct     String?
  image_path    image_path? @relation(fields: [id_image_path], references: [id_image_path], onDelete: Cascade, map: "fk_image_path")

  // @@schema("catho_schema")
}
